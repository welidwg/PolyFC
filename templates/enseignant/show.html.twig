{% extends 'base.html.twig' %}

{% block title %}Enseignant{% endblock %}

{% block body %}
    <h1>Enseignant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ enseignant.id }}</td>
            </tr>
            <tr>
                <th>Matricule</th>
                <td>{{ enseignant.matricule }}</td>
            </tr>
            <tr>
                <th>DateNaissance</th>
                <td>{{ enseignant.dateNaissance ? enseignant.dateNaissance|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ enseignant.sexe }}</td>
            </tr>
            <tr>
                <th>Addresse</th>
                <td>{{ enseignant.Addresse }}</td>
            </tr>
            <tr>
                <th>Cin</th>
                <td>{{ enseignant.cin }}</td>
            </tr>
            <tr>
                <th>Passeport</th>
                <td>{{ enseignant.passeport }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ enseignant.specialite }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_enseignant_index') }}">back to list</a>

    <a href="{{ path('app_enseignant_edit', {'id': enseignant.id}) }}">edit</a>

    {{ include('enseignant/_delete_form.html.twig') }}
{% endblock %}
