{% extends 'base.html.twig' %}

{% block title %}SessionCertif
{% endblock %}

{% block content %}
	{% embed "layout/card.html.twig" %}
		{% block card_title %}
			SessionCertif Details

		{% endblock %}
		{% block card_body %}
			<table class="table">
				<tbody>

					<tr>
						<th>Libelle</th>
						<td>{{ session_certif.libelle }}</td>
					</tr>
					<tr>
						<th>DateDebut</th>
						<td>{{ session_certif.dateDebut ? session_certif.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
					</tr>
					<tr>
						<th>DateFin</th>
						<td>{{ session_certif.dateFin ? session_certif.dateFin|date('Y-m-d H:i:s') : '' }}</td>
					</tr>
				</tbody>
			</table>

		{% endblock %}
		{% block card_footer %}

			<a class="btn btn-primary" href="{{ path('app_session_certif_index') }}">Retour Ã  la liste</a>

			<a class="btn btn-success" href="{{ path('app_session_certif_edit', {'id': session_certif.id}) }}">Modifer</a>


			{{ include('session_certif/_delete_form.html.twig') }}
		{% endblock %}

	{% endembed %}


{% endblock %}
